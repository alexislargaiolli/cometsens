<div class="container">			
	<div class="page-header">
		<h1>
			Admin <small>projects</small>
		</h1>
	</div>	
	<navigation-admin></navigation-admin>

	<div style="padding:20px;">
		<div class="row slideItem" ng-repeat="project in projects">
			<div class="col-md-10"><p>{{project.name}}</p></div>
			<div class="col-md-2">
				<a href="#/project/{{project._id}}" class="btn btn-success">
					<span class="glyphicon glyphicon-pencil"></span>
				</a>
				<button type="button" ng-click="remove(project)" class="btn btn-danger">
					<span class="glyphicon glyphicon-trash"></span>
				</button>
			</div>
		</div>

		<div style="padding:10px;text-align:center;">
			<button type="button" ng-click="create()" class="btn btn-success">
				<span class="glyphicon glyphicon-plus"></span>
			</button>
		</div>
	</div>
</div>